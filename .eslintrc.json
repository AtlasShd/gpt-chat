{
  "env": {
      "node": true,
      "es2022": true,
      "jest": true,
      "browser": true
  },
  "extends": [
      "airbnb-base",
      "plugin:react/recommended"
  ],
  "plugins": [
    "import",
    "jsx-a11y",
    "react",
    "react-hooks"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "ignorePatterns": ["/dist/", "/gulp/", "/node_modules/"],
  "rules": {
    "no-console":"off",
    "import/extensions":"off",
    "max-len":"off",
    "no-unused-vars":"warn",
    "no-trailing-spaces": "warn",
    "no-underscore-dangle":"off",
    "class-methods-use-this":"off",
    "no-param-reassign": "off",
    "no-continue":"off",
    "react/prop-types": "off",
    "no-restricted-globals": "off"
  }
}